			save 'doubleQuote' 0x22

		local yCurlyQuotes [mix quoteBottom quoteTop 0.5]
		local commaLow (DESCENDER * 0.9)
		local ratio : Math.min 1 : (quoteTop - quoteBottom) / (PERIODSIZE - commaLow)

		sketch # lowSingleQuote
