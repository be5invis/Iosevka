					include : HookTopShapeOfG CAP
					save 'Ghooktop.toothed'
				branch
					include : OverlayBarOfG CAP
					save 'Gbar.toothed'

			branch
				include : GShape false CAP SMOOTHA SMOOTHB
				save 'G.toothless'
				branch
					include : HookTopShapeOfG CAP
					save 'Ghooktop.toothless'
				branch
					include : OverlayBarOfG CAP
					save 'Gbar.toothless'


		sketch # smcpG
