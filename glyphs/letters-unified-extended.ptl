		include : VBar MIDDLE y1 y2 MVERTSTROKE
		include : VBar MIDDLE (y2 - HALFSTROKE) CAP
	
		save 'varphi' 0x3D5
	
	sketch # cyref
		include glyphs.varphi AS_BASE
