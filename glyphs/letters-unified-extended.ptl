			include : tagged 'serifMT' : CenterTopSerif MIDDLE CAP JUT
			include : tagged 'serifMB' : CenterBottomSerif MIDDLE DESCENDER JUT
		save 'cyref' 0x444
		save 'latinphi' 0x278

symbol-block 'Psi'
